    "@commitlint/config-conventional": "^17.0.3",
    "@types/react-native": "0.70.7",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@tsconfig/react-native": "^2.0.2",
